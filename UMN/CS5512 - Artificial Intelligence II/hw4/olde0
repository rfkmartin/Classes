%*******************************************************************%
%  Semantics
%*******************************************************************%

s-->np,vp.
s-->s,conjunction,s.

np-->pronoun.
np-->noun.
np-->article,noun.
np-->digit,digit.
np-->noun,pp.
np-->noun,relclause.


vp-->verb.
vp-->verb,np.
vp-->verb,adjective.
vp-->verb,pp.
vp-->verb,adverb.

pp-->preposition,np.

relclause-->[that],vp.



